{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "webRoot": "${workspaceFolder}/frontend",
      "url": "https://localhost:3000/multicloud/welcome"
    },
    {
      "name": "Launch Chrome 9000",
      "request": "launch",
      "type": "chrome",
      // "trace": true,
      "webRoot": "${workspaceFolder}/frontend",
      "url": "http://localhost:9000/multicloud/home/welcome"
    },
    {
      "type": "node",
      "name": "vscode-jest-tests.v2.frontend",
      "request": "launch",
      "args": [
        "--runInBand",
        "--watchAll=false",
        "--coverage=false",
        "--testNamePattern",
        "${jest.testNamePattern}",
        "--runTestsByPath",
        "${jest.testFile}"
      ],
      "cwd": "${workspaceFolder}/frontend",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/frontend/node_modules/.bin/jest"
    },
    {
      "type": "node",
      "name": "vscode-jest-tests.v2.backend",
      "request": "launch",
      "args": [
        "--runInBand",
        "--watchAll=false",
        "--coverage=false",
        "--testNamePattern",
        "${jest.testNamePattern}",
        "--runTestsByPath",
        "${jest.testFile}"
      ],
      "cwd": "${workspaceFolder}/backend",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/backend/node_modules/.bin/jest"
    }
  ]
}
