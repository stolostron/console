        "ramda": "^0.28.0",
        "react-masonry-css": "^1.0.16",
        "string-similarity": "^4.0.4",
        "svg.js": "^2.7.1",
        "uuid": "8.3.2",
        "validator": "^13.7.0",
        "whatwg-fetch": "^3.6.2",
        "yaml": "^1.10.2",
        "yup": "^0.32.11"
