        "jest-fetch-mock": "^3.0.3",
        "jest-raw-loader": "1.0.1",
        "jest-sonar-reporter": "^2.0.0",
        "jest-watch-typeahead": "^1.0.0",
        "json-loader": "^0.5.7",
        "mini-css-extract-plugin": "^2.4.4",
        "monaco-editor-webpack-plugin": "^6.0.0",
