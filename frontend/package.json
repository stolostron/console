    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "project": "./tsconfig.json"
        },
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:jest/recommended",
            "plugin:react-hooks/recommended",
            "prettier"
        ],
        "ignorePatterns": [
            "**/*.stories.tsx"
        ],
        "plugins": [
            "react",
            "@typescript-eslint",
            "jest",
            "react-hooks",
            "prettier"
        ],
        "env": {
            "browser": true,
            "node": true
        },
        "rules": {
            "react/jsx-key": 1,
            "prettier/prettier": "error",
            "@typescript-eslint/no-non-null-assertion": "off",
            "react/react-in-jsx-scope": "off",
            "@typescript-eslint/explicit-module-boundary-types": "off",
            "jest/expect-expect": "off",
            "@typescript-eslint/no-floating-promises": [
                "off",
                {
                    "ignoreVoid": true
                }
            ],
            "@typescript-eslint/no-misused-promises": [
                "error",
                {
                    "checksVoidReturn": false
                }
            ],
            "@typescript-eslint/no-non-null-asserted-optional-chain": "off",
            "jest/no-disabled-tests": "off",
            "no-async-promise-executor": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-empty-function": "off",
            "jest/no-identical-title": "off",
            "@typescript-eslint/ban-types": "off",
            "no-empty": "off",
            "no-constant-condition": "off",
            "jest/no-export": "off",
            "@typescript-eslint/no-extra-semi": "off",
            "@typescript-eslint/no-restricted-imports": [
                "error",
                {
                    "paths": [
                        {
                            "name": "react-i18next",
                            "message": "Please use <Trans /> and useTranslation() from ./src/lib/acm-i18next and avoid other exports from react-i18next"
                        },
                        {
                            "name": "recoil",
                            "message": "Please use hooks from ./src/shared-recoil for ACM/MCE dynamic plugins common RecoilRoot"
                        },
                        {
                            "name": "@patternfly/react-core",
                            "importNames": [
                                "Truncate"
                            ],
                            "message": "Truncate component is not available on OpenShift 4.10; use src/components/Trucate instead"
                        }
                    ],
                    "patterns": [
                        {
                            "group": [
                                "**/atoms",
                                "**/selectors"
                            ],
                            "message": "Please use useSharedAtoms() or useSharedSelectors() from ./src/shared-recoil to access atoms/selectors for ACM/MCE dynamic plugins common RecoilRoot"
                        }
                    ]
                }
            ]
        },
        "overrides": [
            {
                "files": [
                    "**/*.test.ts?"
                ],
                "rules": {
                    "@typescript-eslint/no-restricted-imports": "off"
                }
            }
        ],
        "settings": {
            "react": {
                "version": "detect"
            }
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    }
}
