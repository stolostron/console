        "node-util": "^0.0.1",
        "path-browserify": "^1.0.1",
        "prettier": "^2.4.1",
        "process": "^0.11.10",
        "query-string": "^7.0.1",
        "raw-loader": "^4.0.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-hot-loader": "^4.13.0",
        "react-i18next": "^11.14.2",
        "react-monaco-editor": "^0.46.0",
        "react-redux": "^7.2.6",
        "react-refresh": "^0.11.0",
        "react-refresh-typescript": "^2.0.2",
        "react-router-dom": "^5.3.0",
        "react-tagsinput": "3.19.x",
        "recoil": "^0.5.2",
        "redux": "^4.1.2",
        "redux-thunk": "^2.4.0",
        "reselect": "^4.1.4",
        "stream-browserify": "^3.0.0",
        "style-loader": "^3.3.1",
        "swr": "^1.0.1",
        "temptifly": "^0.5.1",
        "ts-import-plugin": "^1.6.7",
        "ts-jest": "^27.0.7",
        "ts-loader": "^9.2.6",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.11.0",
        "type-fest": "^2.5.4",
        "typescript": "^4.4.4",
        "uuid": "8.3.2",
        "v8-compile-cache": "^2.3.0",
        "webpack": "^5.64.1",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.5.0"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "parserOptions": {
            "project": "./tsconfig.json"
        },
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:jest/recommended"
        ],
        "plugins": [
            "react",
            "@typescript-eslint",
            "jest",
            "react-hooks"
        ],
        "rules": {
            "@typescript-eslint/no-non-null-assertion": "off",
            "react/react-in-jsx-scope": "off",
            "@typescript-eslint/explicit-module-boundary-types": "off",
            "jest/expect-expect": "off",
            "@typescript-eslint/no-floating-promises": [
                "off",
                {
                    "ignoreVoid": true
                }
            ],
            "@typescript-eslint/no-misused-promises": [
                "error",
                {
                    "checksVoidReturn": false
                }
            ],
            "@typescript-eslint/no-non-null-asserted-optional-chain": "off",
            "jest/no-disabled-tests": "off",
            "no-async-promise-executor": "off",
            "@typescript-eslint/no-explicit-any": "off",
            "@typescript-eslint/no-empty-function": "off",
            "jest/no-identical-title": "off",
            "@typescript-eslint/ban-types": "off",
            "no-empty": "off",
            "no-constant-condition": "off",
            "jest/no-export": "off",
            "@typescript-eslint/no-extra-semi": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "printWidth": 120,
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true
    },
    "consolePlugin": {
        "name": "acm-plugin",
        "version": "0.0.0",
        "displayName": "ACM Plugin",
        "description": "Cluster management",
        "exposedModules": {
            "perspective": "./src/perspective",
            "welcome": "./src/routes/Home/Welcome/Welcome.tsx",
            "overview": "./src/routes/Home/Overview/OverviewPlugin.tsx",
            "search": "./src/routes/Home/Search/SearchPlugin.tsx",
            "clusters": "./src/routes/Infrastructure/Clusters/ClustersPlugin.tsx",
            "baremetalassets": "./src/routes/Infrastructure/BareMetalAssets/BareMetalAssetsPlugin.tsx",
            "automations": "./src/routes/Infrastructure/Automations/AutomationsPlugin.tsx",
            "environments": "./src/routes/Infrastructure/InfraEnvironments/InfraEnvironmentsPlugin.tsx",
            "applications": "./src/routes/Applications/ApplicationsPlugin.tsx",
            "governance": "./src/routes/Governance/GovernancePlugin.tsx",
            "credentials": "./src/routes/Credentials/CredentialsPlugin.tsx"
        },
        "dependencies": {
            "@console/pluginAPI": "*"
        }
    }
}
