{
    "name": "frontend",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "TS_NODE_PROJECT=webpack.tsconfig.json webpack serve --mode development --stats-children",
        "build": "TS_NODE_PROJECT=webpack.tsconfig.json webpack --mode production",
        "tsc:noEmit": "tsc --noEmit",
        "clean": "rm -rf coverage build",
        "test": "concurrently --kill-others-on-fail npm:jest npm:eslint npm:prettier npm:tsc:noEmit -c green,blue,cyan,magenta",
        "jest": "jest --runInBand",
        "jest:parallel": "jest",
        "eslint": "eslint src --ext .ts --max-warnings=0",
        "eslint:fix": "eslint src test --ext .ts --fix",
        "prettier": "prettier --check src public",
        "prettier:fix": "prettier --write src public",
        "update": "npx npm-check-updates -x @testing-library/user-event,monaco-editor,eslint,react-router-dom --doctor --upgrade && npm audit fix && npm dedup",
        "plugin:build": "CONSOLE_PLUGIN_SKIP_EXT_VALIDATOR=true TS_NODE_PROJECT=webpack.tsconfig.json webpack -c webpack.plugin.ts --mode production",
        "plugin:watch": "CONSOLE_PLUGIN_SKIP_EXT_VALIDATOR=true TS_NODE_PROJECT=webpack.tsconfig.json webpack -c webpack.plugin.ts --mode production --watch",
        "plugin:serve": "npx http-server ./dist -p 3000 -c-1 --cors",
        "plugin": "TS_NODE_PROJECT=webpack.tsconfig.json webpack serve -c webpack.plugin.ts --mode production",
        "reinstall": "rm -rf node_modules package-lock.json && npm install && npm audit fix && npm dedup"
    },
    "dependencies": {
        "@apollo/client": "^3.4.17",
        "@kubernetes/client-node": "^0.16.0",
        "@open-cluster-management/ui-components": "^1.25.0",
        "@patternfly/react-code-editor": "^4.9.8",
        "@patternfly/react-core": "^4.168.8",
        "@patternfly/react-icons": "^4.19.8",
        "@patternfly/react-table": "^4.37.8",
        "@redhat-cloud-services/rule-components": "^3.2.3",
        "@reduxjs/toolkit": "1.6.x",
        "axios": "^0.24.0",
        "graphql": "^16.0.1",
        "handlebars": "^4.7.7",
        "i18next": "^21.5.2",
        "i18next-browser-languagedetector": "^6.1.2",
        "i18next-http-backend": "^1.3.1",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "monaco-editor": "^0.27.0",
        "openshift-assisted-ui-lib": "1.5.44",
        "react-masonry-css": "^1.0.16",
        "uuid": "8.3.2",
        "validator": "^13.7.0",
        "whatwg-fetch": "^3.6.2",
        "yaml": "^1.10.2",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@openshift-console/dynamic-plugin-sdk": "0.0.3",
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-react": "^7.16.0",
        "@babel/preset-typescript": "^7.16.0",
        "@openshift-console/dynamic-plugin-sdk-webpack": "0.0.4",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
        "@sentry/browser": "6.15.0",
        "@svgr/webpack": "^5.5.0",
        "@testing-library/jest-dom": "^5.15.0",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^12.8.3",
        "@types/compression-webpack-plugin": "^9.0.0",
        "@types/copy-webpack-plugin": "^8.0.1",
        "@types/css-minimizer-webpack-plugin": "^3.0.2",
        "@types/get-value": "^3.0.2",
        "@types/jest": "^27.0.2",
        "@types/js-yaml": "^4.0.4",
        "@types/lodash": "^4.14.177",
        "@types/mini-css-extract-plugin": "^2.4.0",
        "@types/node": "^16.11.7",
        "@types/react": "^17.0.35",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@types/testing-library__jest-dom": "^5.14.1",
        "@types/validator": "^13.7.0",
        "@types/webpack-dev-server": "^4.5.0",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "babel-jest": "^27.3.1",
        "babel-loader": "^8.2.3",
        "browserify-fs": "^1.0.0",
        "buffer": "^6.0.3",
        "bundle-loader": "^0.5.6",
        "compression-webpack-plugin": "^9.0.1",
        "concurrently": "^6.4.0",
        "copy-webpack-plugin": "^10.0.0",
        "crypto": "^1.0.1",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.1.4",
        "eslint": "^7.32.0",
        "eslint-plugin-jest": "^25.2.4",
        "eslint-plugin-react": "^7.27.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "file-loader": "^6.2.0",
        "handlebars-loader": "^1.7.1",
        "html-webpack-plugin": "^5.5.0",
        "i18next-parser": "^5.2.0",
