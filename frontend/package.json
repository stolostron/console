{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": ". ../port-defaults.sh && TS_NODE_PROJECT=webpack.tsconfig.json webpack serve --mode development --stats-children",
    "start:hot": "TS_NODE_PROJECT=webpack.tsconfig.json webpack serve --mode development --hot=true --stats-children",
    "launch": ". ../port-defaults.sh && TS_NODE_PROJECT=webpack.tsconfig.json webpack serve --mode development --env LAUNCH --stats-children",
    "storybook": "storybook dev",
    "build": "NODE_OPTIONS=--max-old-space-size=8192 TS_NODE_PROJECT=webpack.tsconfig.json webpack --mode production",
    "tsc": "tsc --noEmit",
    "tsc:all": "npm run tsc --workspaces --include-workspace-root --if-present",
    "build:plugin": "npm run build:workspaces && ./scripts/checkPlugin.sh && cd plugins/${PLUGIN} && cp ../../package.json . && NODE_OPTIONS=--max-old-space-size=4096 TS_NODE_PROJECT=../../webpack.tsconfig.json webpack -c webpack.plugin.ts --env plugin=$PLUGIN --mode production",
    "build:plugin:acm": "PLUGIN=acm npm run build:plugin",
    "build:plugin:mce": "PLUGIN=mce npm run build:plugin",
    "build:workspaces": "npm run build --workspaces --if-present",
    "check": "npm run i18n && npm run check-doc --workspaces --include-workspace-root --if-present && npm run prettier:all && npm run lint:all && npm run tsc:all",
    "check:fix": "npm run i18n:fix && npm run check-doc:fix --workspaces --include-workspace-root --if-present && npm run prettier:fix:all && npm run lint:fix:all && npm run tsc:all",
    "clean": "rm -rf coverage build",
    "test": "npm run jest --",
    "jest": "TZ=UTC jest",
    "jest:update-snapshot": "npm run jest -- --updateSnapshot",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:all": "npm run lint --workspaces --include-workspace-root --if-present",
    "lint:fix": "npm run lint -- --fix",
    "lint:fix:all": "npm run lint:fix --workspaces --include-workspace-root --if-present",
    "postinstall": "npm run build:workspaces",
    "prettier": "prettier --check src public plugins",
    "prettier:all": "npm run prettier --workspaces --include-workspace-root --if-present",
    "prettier:fix": "prettier --write src public plugins",
    "prettier:fix:all": "npm run prettier:fix --workspaces --include-workspace-root --if-present",
    "i18n": "i18next --fail-on-warnings && git diff --exit-code public/locales/en/translation.json",
    "i18n:fix": "i18next --fail-on-warnings",
    "i18n-generate": "rm -rf public/locales/upload && MODE=generate i18next --fail-on-warnings",
    "i18n-upload": "i18n-scripts/memsource-upload.sh",
    "i18n-download": "i18n-scripts/memsource-download.sh",
    "update": "npx npm-check-updates -x @testing-library/user-event,monaco-editor,eslint,react-router-dom --doctor --upgrade && npm audit fix && npm dedup",
    "serve:plugins": "concurrently npm:serve:plugin:* -c green,blue,cyan,magenta",
    "serve:plugin": "./scripts/checkPlugin.sh && cd plugins/${PLUGIN} && cp ../../package.json . && TS_NODE_PROJECT=../../webpack.tsconfig.json webpack serve -c webpack.plugin.ts --env plugin=$PLUGIN --env port=$PORT --mode development",
    "serve:plugin:acm": ". ../port-defaults.sh && PLUGIN=acm PORT=${ACM_PORT} npm run serve:plugin",
    "serve:plugin:mce": ". ../port-defaults.sh && PLUGIN=mce PORT=${MCE_PORT} npm run serve:plugin"
  },
