        "@stolostron/ui-components": "^1.37.0",
        "ajv": "^8.10.0",
        "axios": "^0.26.0",
        "cytoscape": "3.21.0",
        "cytoscape-cola": "^2.5.0",
        "d3": "^7.3.0",
        "d3-selection": "^3.0.0",
        "deep-diff": "^1.0.2",
