-include /opt/build-harness/Makefile.prow

.PHONY: build
build:
	npm run build

.PHONY: install
install:
	npm ci
	npm run postinstall

.PHONY: check
check:
	npm run check

.PHONY: lint
lint:
	npm run lint

.PHONY: unit-tests
unit-tests:
	if [ ! -d "test-output" ]; then \
		mkdir test-output; \
	fi
	npm run test
